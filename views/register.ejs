        color: white;
      }

      .about {

        color: white;
        font-family: 'Akshar-Variable';
        font-size: 90px;
        margin-top: 190px;

      }

      .container-flex {
        display: flex;
        flex-wrap: wrap;
        gap: 127px;
        justify-content: center;
        margin-top: 70px
      }

      .small {
        height: 599px;
        width: 318px;
        border-radius: 5px;
        background-color: #2A3142;



      }

      .newaccount {
        font-size: 20px;
        color: #8B88FC;
        font-family: 'Akshar-Variable';
        text-align: center;
        position: relative;
        left: 720px;
        top: 15px;


      }

      .label1 {
        color: white;
        position: relative;
        left: 200px;
        top: 60px;
        font-size: 20px;

      }

      .label2 {
        color: white;
        position: relative;
        left: 455px;
        top: 60px;
        font-size: 20px;

      }

      .label3 {
        color: white;
        position: relative;
        left: 780px;
        top: 60px;
        font-size: 20px;

      }

      .inputs {
        width: 290px;
        height: 36px;
        flex-shrink: 0;
        border-radius: 10px;
        background: #D9D9D9;
        margin: 13px;
      }

      .labels {
        color: aliceblue;
        font-size: 15px;
        position: relative;
        top: 10px;
        left: 12px;
      }

      .labelsi {
        color: aliceblue;
        font-size: 15px;
        position: relative;
        top: 10px;
        left: 12px;
      }

      .labelsi:after {
        content: " *";
        color: red;
      }

      .registerbtn {
        width: 150px;
        height: 53px;
        flex-shrink: 0;
        border-radius: 20px;
        background: #08A2A8;
        margin-top: 25px;

      }
