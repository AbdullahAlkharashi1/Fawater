      }

      #navo:hover {
        opacity: 100%;
        transition: background-color 0.9s ease;

      }

      #log {
        color: rgb(47, 49, 12);
      }

      .inc {
      color: #ffffff;
      font-size: 40px;
      position: absolute;
      left: 1%;
      font-family: "Montserrat", "Helvetica", Arial, sans-serif;
      top: 6px;
      user-select: none !important;
      font-weight: bolder;
    }

      a {
        font-family: Oswald;
        font-size: 20px !important;
        user-select: none !important;
        color: white !important;
        margin-left: 10px;
        margin-right: 10px;

      }

      .navbar-collapse {
        left: 200px !important;
      }

     .logDiv {
      position: relative;
      left: 9px;
      background-color: #16676B;
