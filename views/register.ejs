    <style>
      html,
      body {

        background: #252735;
        font-family: 'Akshar-Variable';

      }

      .container-fluid {
        background-color: #36394C;


      }

      .navbar {

        padding: 0 !important;
      }

      #navo {
        color: #36394C;
